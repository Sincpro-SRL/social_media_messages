<odoo>
    <record id="view_move_form_whatsapp_button" model="ir.ui.view">
        <field name="name">view.move.form.whatsapp.button</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form" />
        <field name="arch" type="xml">
            <xpath expr="//form//header//button[@name='preview_invoice']" position="after">
                <button type="object" name="traceability_whatsapp" string="Enviar por Whatsapp" />
            </xpath>
        </field>
    </record>
</odoo>